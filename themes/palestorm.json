{
  "name": "Palestorm",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "parameter.declaration": {
      "foreground": "#e7b872"
    },
    "parameter": {
      "foreground": "#cfc9c2"
    },
    "property.declaration": {
      "foreground": "#C0CAF5"
    },
    "property.defaultLibrary": {
      "foreground": "#C0CAF5"
    },
    "*.defaultLibrary": {
      "foreground": "#82AAFF"
    },
    "variable.defaultLibrary": {
      "foreground": "#C0CAF5"
    },
    "variable.declaration": {
      "foreground": "#82AAFF"
    },
    "variable": {
      "foreground": "#c0caf5"
    }
  },
  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "foreground": "#bfc6e6"
      }
    },
    {
      "name": "Comment",
      "scope": "comment",
      "settings": {
        "foreground": "#697098",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "String Quoted",
      "scope": "string.quoted",
      "settings": {
        "foreground": "#C3E88D"
      }
    },
    {
      "name": "String Unquoted",
      "scope": "string.unquoted",
      "settings": {
        "foreground": "#bfc6e6"
      }
    },
    {
      "name": "Support Constant Math",
      "scope": "support.constant.math",
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric", "constant.character.numeric"],
      "settings": {
